class A {}

static var global: A = new A {}

interface TestInterface {
    function test(let this, a: A);
}

class B {}
class C {}

implements TestInterface for B {
    function test(let this, a: A) {}
}
implements TestInterface for C {
    function test(let this, a: A) {
        global = a
    }
}

function main() {
    let a = new A {}
    let b = new B {}
    let c = new C {}

    b.test(a)
    c.test(a)
}
