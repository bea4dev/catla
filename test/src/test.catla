import std::operator::add::Add

struct Value<T> {
    let value: T
}

implements<T> Add<T, T> for Value<T> where T: Add<T, T> {
    override function add(let this, other: T) -> T {
        let a = this
        let a = this.value
        let b = other
        return b
    }
}

